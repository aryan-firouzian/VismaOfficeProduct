@model VismaOfficeProduct.Models.RebateVolume
@* Edit partial view for RebateVolume model called by selection change of dropdownlist of the rebate type *@


<div id="RebateCreate">
    <fieldset>
        <legend>RebateVolume</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.Quantity)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Quantity)
            @Html.ValidationMessageFor(model => model.Quantity)
        </div>

        @Html.HiddenFor(model => model.RebateId)

        <div class="editor-label">
            @Html.LabelFor(model => model.Percentage)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Percentage)<span> %</span>
            @Html.ValidationMessageFor(model => model.Percentage)
        </div>
    </fieldset>
</div>
